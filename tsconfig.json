{
    "compilerOptions": {
        "outDir": "./build/",
        "target": "es6",
        "allowJs": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": false,
        "jsx": "react",
        "lib": [
            "es2015",
            "dom",
            "webworker"
        ],
        "skipLibCheck": false
    }
}
  